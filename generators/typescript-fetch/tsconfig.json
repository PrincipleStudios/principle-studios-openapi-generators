{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "esnext", "DOM"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["node", "jest"],
    "declaration": false,
    "rootDir": ".",
    "paths": {
      "@principlestudios/openapi-codegen-typescript-fetch": ["./src"],
    }
  }
}
