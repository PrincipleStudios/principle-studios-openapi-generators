{
  "name": "@principlestudios/openapi-codegen-typescript-rxjs",
  "version": "0.2.0",
  "description": "A typescript client code generator for principled development using RxJS",
  "scripts": {
    "test": "jest",
    "test:update": "jest -u",
    "pretest": "npm run build",
    "build": "openapi-codegen-typescript ../../../schemas/no-refs.yaml tests/no-refs/ && openapi-codegen-typescript ../../../schemas/petstore.yaml tests/petstore/ && openapi-codegen-typescript ../../../schemas/headers.yaml tests/headers/",
    "tsc": "tsc --project tsconfig.build.json",
    "prepack": "npm run tsc"
  },
  "author": "Matt DeKrey",
  "license": "BSD-2-Clause",
  "homepage": "https://github.com/mdekrey/principle-studios-openapi-generators",
  "files": [
    "lib/*",
    "readme.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mdekrey/principle-studios-openapi-generators.git",
    "directory": "generators/typescript-rxjs"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@principlestudios/openapi-codegen-typescript": "^0.2.0",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/eslint": "^7.28.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.0",
    "typescript": "^4.4.3"
  }
}
