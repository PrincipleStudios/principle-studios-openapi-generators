{{>partial_header Meader}}
{{#with Model as |Model|}}
{{#each Imports}}
import {
{{#each Members}}
    {{{.}}},
{{/each}}
} from '{{Path}}';
{{/each}}

/**{{#if Description}}
 * {{{Description}}}{{/if}}
 * @export
 * @interface {{ClassName}}
 */
export type {{ClassName}} =
{{#each TypeEntries as |TypeEntry|}}
{{#if Model.DiscriminatorProperty}}
    | ({ '{{{Model.DiscriminatorProperty}}}': '{{{TypeEntry.DiscriminatorValue}}}' } & {{{TypeEntry.TypeName}}})
{{else}}
    | {{{TypeEntry.TypeName}}}
{{/if}}
{{/each}}
;
{{/with}}
